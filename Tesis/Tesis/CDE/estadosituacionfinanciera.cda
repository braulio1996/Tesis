      <Columns/>
      <Parameters>
         <Parameter default="fecha1" name="fecha1" type="String"/>
         <Parameter default="fecha2" name="fecha2" type="String"/>
      </Parameters>
      <Query><![CDATA[select  distinct eri.detalle as " ",(select  round(sum(a.debe::numeric)-sum(a.haber::numeric),2) as " " from estado_resultado_integral as a where a.id=eri.id)
from estado_resultado_integral as eri join scafp_conta_cuenta as scc on scc.id=eri.codigo_contable
where scc.descripcion ='Efectivo Equivalente al efectivo' and eri.fecha BETWEEN ${fecha1} AND ${fecha2};]]></Query>
   </DataAccess>
