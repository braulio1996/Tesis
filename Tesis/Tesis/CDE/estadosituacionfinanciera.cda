   <DataAccess access="public" connection="Querry_efectivo" id="Querry_efectivo" type="sql">
      <Name>Querry_efectivo</Name>
      <Cache duration="3600" enabled="false"/>
      <Columns/>
      <Parameters>
         <Parameter default="fecha1" name="fecha1" type="String"/>
         <Parameter default="fecha2" name="fecha2" type="String"/>
         <Parameter default="efectivo_equivalente" name="efectivo_equivalente" type="String"/>
      </Parameters>
      <Query><![CDATA[select round(sum(eri.debe::numeric)-sum(eri.haber::numeric),2) as "Efectivo Equivalente al efectivo"
from estado_resultado_integral as eri join scafp_conta_cuenta as scc on scc.id=eri.codigo_contable
where scc.descripcion ='Efectivo Equivalente al efectivo'
and eri.fecha BETWEEN ${fecha1} AND ${fecha2};]]></Query>
   </DataAccess>
   <DataAccess access="public" connection="Querry_efectivo_d" id="Querry_efectivo_d"
               type="sql">
